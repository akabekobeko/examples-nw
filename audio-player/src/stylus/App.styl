@import "CommonColor.styl"

/**
 * ページ全体。
 */
body, html {
  width 100%
  height 100%
  margin 0
  padding 0
}

/**
 * アプリケーション部分。
 */
.app {
  position relative
  width 100%
  height 100%
  cursor default
  font-family sans-serif
  color color_adule
}

/**
 * 選択不可。
 */
.unselectable {
  -webkit-touch-callout none
  -webkit-user-select none
  -khtml-user-select none
  -moz-user-select none
  -ms-user-select none
  user-select none
}